{
  "name": "dicom-toolkit",
  "version": "0.1.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "mutation:quick": "stryker run --mutate 'src/**/*.ts' --timeoutFactor 1.5 --concurrency 2 --testRunner vitest --fileLogLevel error",
    "mutation": "stryker run",
    "check:reqs": "node tools/check_requirements.mjs"
  },
  "dependencies": {
    "dcmjs": "^0.45.0",
    "jszip": "^3.10.1",
    "zod": "^4.1.10"
  },
  "devDependencies": {
    "@stryker-mutator/core": "^9.1.1",
    "@stryker-mutator/typescript-checker": "^9.1.1",
    "@stryker-mutator/vitest-runner": "^9.1.1",
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "@vitejs/plugin-react": "^5.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "fast-check": "^4.3.0",
    "fast-glob": "^3.3.3",
    "js-yaml": "^4.1.0",
    "jsdom": "^27.0.0",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.6",
    "vitest": "^3.2.4"
  }
}